<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:viewmodels="clr-namespace:AlgimedWPFApp.ViewModels"
        x:Class="AlgimedWPFApp.Views.RegisterView"
		Title="Register" Height="450" Width="800">
	<StackPanel>
		<TextBlock>Login</TextBlock>
		<TextBox Text="{Binding Login, Mode=OneWayToSource}" />
		<TextBlock>Password</TextBlock>
		<TextBox Text="{Binding Password, Mode=OneWayToSource}" />
        <Button Content="Register" Command="{Binding RegisterCommand}" />
    </StackPanel>
</Window>